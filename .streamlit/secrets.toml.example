# .streamlit/secrets.toml.example
# Copy to .streamlit/secrets.toml and fill in real values (DO NOT COMMIT secrets.toml)

[auth]
redirect_uri = "https://your-app-name.streamlit.app/oauth2callback"  # Update for your deployed app
cookie_secret = "YOUR_RANDOM_32_CHAR_STRING"
client_id = "YOUR_LINKEDIN_CLIENT_ID"
client_secret = "YOUR_LINKEDIN_CLIENT_SECRET"
server_metadata_url = "https://www.linkedin.com/oauth/v2/openid/.well-known/openid_configuration?partner=linkedin"

[aws]
region = "us-east-1"
# access_key_id = "YOUR_AWS_ACCESS_KEY"  # Optional, prefer ~/.aws/credentials
# secret_access_key = "YOUR_AWS_SECRET_KEY"